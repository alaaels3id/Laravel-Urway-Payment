{
    "name": "alaaelsaid/laravel-urway-payment",
    "description": "This package is to integrate with URWay payment",
    "type": "library",
    "keywords": ["payment", "urway"],
    "require": {
        "php": "^7.4"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Alaaelsaid\\LaravelUrwayPayment\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Alaa Elsaid",
            "email": "alaa4cat@gmail.com"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Alaaelsaid\\LaravelUrwayPayment\\Providers\\UrwayServiceProvider"
            ],
            "aliases": {
                "Urway": "Alaaelsaid\\LaravelUrwayPayment\\Facade\\Urway"
            }
        }
    },
    "prefer-stable": true,
    "minimum-stability": "stable"
}
